    <!--Modal for province-->
    <% if (pageData.provinceData) { %>
    <div class="provinceDetailPanel modal fade" id="provinces" tabindex="-1" role="dialog" aria-labelledby="failLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <button type="button" class="close closeButton" data-dismiss="modal" aria-label="Close">
                    <img src="public/assets/x.svg" width="24">
                </button>
                <%- pageData.provinceData.housing %>
            </div>
        </div>
    </div>
    <% } %>

    <!--Modal for Industrial Park-->
    <% if (pageData.industrialParkData) { %>
    <div class="provinceDetailPanel modal fade" id="industrialPark" tabindex="-1" role="dialog" aria-labelledby="failLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <button type="button" class="close closeButton" data-dismiss="modal" aria-label="Close">
                    <!-- <img src="public/assets/x.svg" width="24"> -->
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4><%= pageData.industrialParkData.name %></h4>
                <% if (language == "vi") { %> 
                    <p class="onUpdate text-center colorF90"><%= langData.lbl_notVN %></p>
                <% } %>
                <% if (pageData.industrialParkData.owner) { %>                               
                    <h6><%= langData.lbl_investor %></h6>
                    <p><%- pageData.industrialParkData.owner %></p>
                <% } %>
                
                <% if (pageData.industrialParkData.address && pageData.industrialParkData.address.raw) { %>               
                    <h6><%= langData.lbl_address_ip %></h6>
                    <p><%= pageData.industrialParkData.address.raw %></p>
                <% } %>
                
                <% if (pageData.industrialParkData.chinhSachUuDai) { %>               
                    <h6><%= langData.lbl_policies %></h6>
                    <p><%- pageData.industrialParkData.chinhSachUuDai %></p>
                <% } %>
                <% if (pageData.industrialParkData.nganhNgheTiepNhan) { %>               
                    <h6><%= langData.lbl_scope %></h6>
                    <p><%- pageData.industrialParkData.nganhNgheTiepNhan %></p>
                <% } %>
                <% if (pageData.industrialParkData.area) { %>                               
                    <h6><%= langData.lbl_totalArea %></h6>
                    <p><%= pageData.industrialParkData.area %> ha</p>
                <% } %>
                <% if (pageData.industrialParkData.logoImageId) { %>
                    <img src="http://ip.nso.com.vn/HinhBDS/Upload/<%= pageData.industrialParkData.logoImageId %>">
                <% } %>
            </div>
        </div>
    </div>
    <% } %>

    <!--Modal for Association-->
    <% if (pageData.associationData) { %>
    <div class="provinceDetailPanel modal fade" id="association" tabindex="-1" role="dialog" aria-labelledby="failLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <button type="button" class="close closeButton" data-dismiss="modal" aria-label="Close">
                    <!-- <img src="public/assets/x.svg" width="24"> -->
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4><%= pageData.associationData.name %></h4>
                <% if (language == "vi") { %> 
                    <p class="onUpdate text-center colorF90"><%= langData.lbl_notVN %></p>
                <% } %>
                <% if (pageData.associationData.address && pageData.associationData.address.raw) { %>                               
                    <h6><%= langData.lbl_address_ip %></h6>
                    <p class="font-light large"><%= pageData.associationData.address.raw %></p>
                <% } %>
                
                <% if (pageData.associationData.phoneNumber && pageData.associationData.phoneNumber.length > 0) { %>                               
                    <h6><%= langData.lbl_phoneNumber_ip %></h6>
                    <p class="font-light large color333"><%- pageData.associationData.phoneNumber.map(e => '<a class="color333" href="tel:' + e + '">' + e + '</a>').join(", ") %></p>
                <% } %>
                
                <% if (pageData.associationData.email && pageData.associationData.email.length > 0) { %>                               
                    <h6><%= langData.lbl_email %></h6>
                    <p class="font-light large color333"><%- pageData.associationData.email.map(e => '<a class="color333" href="mailto:' + e + '">' + e + '</a>').join(", ") %></p>
                <% } %>
                
                <% if (pageData.associationData.website) { %>                                               
                    <h6><%= langData.lbl_website %></h6>
                    <p class="font-light large color333"><a class="color333" href="<%= pageData.associationData.website %>"><%= pageData.associationData.website %></a></p>
                <% } %>
                
                <% if (pageData.associationData.desc) { %>                               
                    <h6><%= langData.lbl_introduction_ip %></h6>
                    <p class="font-light large color333"><%- pageData.associationData.desc %></p>
                <% } %>
                
                <% if (pageData.associationData.foundDate) { %>                               
                    <h6><%= langData.lbl_foundDate_ip %></h6>
                    <p class="font-light large color333"><%= moment(pageData.associationData.foundDate).format('DD/MM/YYYY') %></p>
                <% } %>
                <% if (pageData.associationData && pageData.associationData.logoImageId) { %>
                    <img src="<%= pageData.associationData.logoImageId.url %>">
                <% } %>
            </div>
        </div>
    </div>
    <% } %>
    
     <!--Modal for News-->
     <% if (pageData.newsData && pageData.newsData.rows && pageData.newsData.rows.length > 0) { 
        for (var e in pageData.newsData.rows) { %>
            <div class="newsPanel modal fade" id="news<%= e %>" tabindex="-1" role="dialog" aria-labelledby="failLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <button type="button" class="close closeButton" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                        <h6 class="newsPanelTitle"><%= pageData.newsData.rows[e].title %></h4>
                        <span><i class="fa fa-clock"></i> <%= moment(pageData.newsData.rows[e].date).format('DD/MM/YYYY') %></span>
                        <div class="newsDivider"></div>
                        <div class="newsPanelContent">
                            <%- pageData.newsData.rows[e].content %>
                        </div>
                        
                    </div>
                </div>
            </div>
    <% } } %>

    <!-- Modal download app -->
    <div class="modal fade" id="messengerApp" tabindex="-1" role="dialog" aria-labelledby="messengerLabel" aria-hidden="true">
        <div class="modal-dialog" role="document" style="width:400px;">
            <div class="modal-content">
                <div class="modal-header" style="border:none;">
                    <h6 class="modal-title" id="messengerLabel"></h6>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body text-center">
                    <img src="public/assets/gp-4.png"/>
                    <h2 class="color1046f1"><%= langData.lbl_please %></h2>
                    <p class="color333"><%= langData.lbl_open %></p>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
    <!--End download app-->

      <!-- Modal download app -->
      <div class="modal fade" id="appDownload" tabindex="-1" role="dialog" aria-labelledby="appDownloadLabel" aria-hidden="true">
        <div class="modal-dialog" role="document" style="width:400px;">
            <div class="modal-content">
                <div class="modal-header" style="border:none;">
                    <h6 class="modal-title" id="appDownloadLabel"></h6>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body text-center">
                    <img src="public/assets/ios.png"/>
                    <h2 class="color1046f1"><%= langData.lbl_hello %></h2>
                    <p class="color333"><%= langData.lbl_scan %></p>
                    <p class="text-center font-bold color000"><%= langData.lbl_or %></p>
                    <p class="color333"><%= langData.lbl_clickDownload %></p>
                    <a class="appLink" href="#"><img src="public/images/app-store.png"></a>
                    <a class="appLink" href="#"><img src="public/images/google-play.png"></a>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
    <!--End download app-->

    <%- include("partials/footers/default") %>
    <script src="public/js/jquery.touchSwipe.min.js"></script>
</body>

</html>
