                                <script>
                                    $(()=> {
                                        var markers = [];
                                        var directionsService = new google.maps.DirectionsService;
                                        var directionsDisplay = new google.maps.DirectionsRenderer({ polylineOptions:{strokeColor:"red",strokeWeight:3}, suppressMarkers:true });
                                        var mainMarker;

                                        function initMapNearBy() {
                                            var lat = parseFloat("<%= pageData.address.latLng.lat %>");
                                            var lng = parseFloat("<%= pageData.address.latLng.lng %>");

                                            
                                            var mapNearby = new google.maps.Map(document.getElementById('mapNearby'), {
                                                zoom: 13,
                                                center: {lat: lat, lng: lng},
                                                maxZoom: 14
                                            });
                                            directionsDisplay.setMap(mapNearby);

                                            mainMarker = new google.maps.Marker({
                                                position: new google.maps.LatLng(lat, lng),
                                                title: "<%= pageData.name %>"
                                            });
                                            mainMarker.setMap(mapNearby);

                                            inputSearchNearBy = document.getElementById('inputBarNearBy');
                                            
                                            var circle = new google.maps.Circle({radius: 50000, center: mainMarker.position});

                                            var options = {
                                                componentRestrictions: {country: 'vn'},
                                                bounds: circle.getBounds()
                                            };

                                            var autocomplete = new google.maps.places.Autocomplete(inputSearchNearBy, options);
                                            autocomplete.addListener('place_changed', function() {
                                                var place = autocomplete.getPlace();
                                                if (!place.geometry) {
                                                    window.alert("No details available for input: '" + place.name + "'");
                                                    return;
                                                }
                                                else {
                                                    DrawDirection(place.geometry.location, function(data) {
                                                        var marker = new google.maps.Marker({
                                                            position: new google.maps.LatLng(place.geometry.location.lat(), place.geometry.location.lng()),
                                                            title: place.name,
                                                            icon: {
                                                                url: "public/assets/marker_normal.png",
                                                                size: new google.maps.Size(32, 50)
                                                            },
                                                            map: mapNearby
                                                        });

                                                        var tr = `<tr>
                                                            <td> <a class="keyLocation" data="`+ place.geometry.location.lat() + "," + place.geometry.location.lng() + `">` + place.name + `</a></td>
                                                            <td>` + data + `</td>
                                                        </tr>`
                                                        
                                                        $('#searchNearPlace').append(tr);

                                                        $('#searchNearPlace').ready(function () {
                                                            $('#searchNearPlace').find(".keyLocation").last().on("click", function () {
                                                                var that = this;
                                                                DrawDirection($(this).attr('data'), function(data) {
                                                                    $(that).next().text(data);
                                                                });
                                                            })
                                                        })
                                                    });
                                                }
                                            });
                                            
                                        }
                                        
                                        google.maps.event.addDomListener(window, "load", initMapNearBy);
                                            
                                        function DrawDirection(destination, callback) {
                                                
                                            directionsService.route({
                                                origin: mainMarker.position,
                                                destination: destination,
                                                travelMode: 'DRIVING'
                                                }, function(response, status) {
                                                if (status === 'OK') {
                                                    directionsDisplay.setDirections(response);
                                                    $('#inputBarNearBy').val(null);
                                                    callback(response.routes[0].legs[0].distance.text);
                                                } else {
                                                    window.alert('Directions request failed due to ' + status);
                                                }
                                            });
                                        }

                                        $('.keyLocation').click(function(){ 
                                            var that = this;
                                            DrawDirection($(this).attr('data'), function(data) {
                                                $(that).next().text(data);
                                            });
                                        });
                                    })
                                </script>
