    <div id="companyBanner">
        <div class="container">
            <% if (pageData.cre_ts && pageData.cre_ts.getTime() % 3 == 0) { %> 
                <img class="profileTopBanner" src="public/images/03.png">
            <% } else if (pageData.cre_ts && pageData.cre_ts.getTime() % 3 == 1) { %>
                <img class="profileTopBanner" src="public/images/02.png">
            <% } else { %> 
                <img class="profileTopBanner" src="public/images/04.png">
            <% } %>
            <!-- <% if (pageData.bannerImageId && pageData.bannerImageId.url) { %>
                <img class="profileTopBanner" src="<%= pageData.bannerImageId.url %>">
            <% } else { %>
                <img class="profileTopBanner" src="public/assets/bernard-hermant-616385-unsplash.jpg">
            <% } %> -->
            <div class="profileTopBanner overlayProfileBanner"></div>
            <div class="companyIntroduce">
                <div class="companyIntroAvatar">
                    <% if (pageData.logoImageId && pageData.logoImageId.url) { %>
                        <img src="<%= pageData.logoImageId.url %>" />
                    <% } else { %>
                        <img src="public/images/avatar.png" />
                    <% } %>
                </div>
                <div class="companyName">
                    <h6 class="fontsize16"><%= pageData.name %>
