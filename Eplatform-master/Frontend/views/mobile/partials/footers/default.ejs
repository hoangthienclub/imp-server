    <script src="public/js/bootstrap.min.js"></script>
    <script src="public/js/fontawesome-all.js"></script>
    <script src="public/slick/slick.js"></script>
    <script src="public/js/mobile.js"></script>
    <script src="public/frontend/main.js"></script>