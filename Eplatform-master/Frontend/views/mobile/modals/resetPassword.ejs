
<!-- Reset Password -->

<div class="modal fade" id="reset" tabindex="-1" role="dialog" aria-labelledby="forgotLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modalLeft">
                <div class="modal-header">
                    <h6 class="modal-title fontsize16" id="forgotLabel" style="display: contents;"><%= langData.title_resetPassword %></h6>
                    <button type="button" class="close closeButton" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group">
                            <label for="otp" class="col-form-label"><%= langData.lbl_otp %></label>
                            <input type="text" class="" id="otp" placeholder="<%= langData.lbl_otp %>" />
                        </div>
                        <div class="form-group">
                            <label for="newPass" class="col-form-label"><%= langData.lbl_newPass %></label>
                            <input type="password" class="" id="newPass" />
                        </div>
                        <div class="form-group">
                            <label for="renewPass" class="col-form-label"><%= langData.lbl_newPassAgain %></label>
                            <input type="password" class="" id="renewPass" />
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <div class="clearfix"></div>
                    <button type="button" class="nextButton"><%= langData.btn_resetPassword %></button>

                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Reset Password -->
