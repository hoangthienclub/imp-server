
<!-- Enter OTP -->

<div class="modal fade" id="confirmOTP" tabindex="-1" role="dialog" aria-labelledby="confirmOTPLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modalLeft">
                <div class="modal-header">
                    <h6 class="modal-title fontsize16" id="confirmOTPLabel" style="display: contents;"><%= langData.lbl_confirmAccount %></h6>
                    <button type="button" class="close closeButton" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group">
                            <label for="otp" class="col-form-label">OTP</label>
                            <input type="text" class="" id="otp" placeholder="<%= langData.lbl_otpInput %>" />
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <div class="clearfix"></div>
                    <button type="button" class="nextButton"><%= langData.lbl_confirm %></button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Enter OTP -->