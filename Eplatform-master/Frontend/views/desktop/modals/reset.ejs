
<!-- Reset Password -->

<div class="modal fade" id="reset" tabindex="-1" role="dialog" aria-labelledby="forgotLabel" aria-hidden="true">
    <div class="modal-dialog" role="document" style="margin-top:20px;">
        <div class="modal-content" style="padding:20px 40px;">
            <div class="modal-header">
                <h6 class="modal-title" id="forgotLabel" style="display: contents;">Reset Password</h6>
                <button type="button" class="close closeButton" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group">
                        <label for="otp" class="col-form-label">OTP</label>
                        <input type="text" class="" id="otp" placeholder="OTP has been sent to your mobile" />
                    </div>
                    <div class="form-group">
                        <label for="newPass" class="col-form-label">New Password</label>
                        <input type="password" class="" id="newPass" />
                    </div>
                    <div class="form-group">
                        <label for="renewPass" class="col-form-label">Retype New Password</label>
                        <input type="password" class="" id="renewPass" />
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <div class="clearfix"></div>
                <button type="button" class="nextButton">Reset Password</button>

            </div>
        </div>
    </div>
</div>
<!-- End Reset Password -->