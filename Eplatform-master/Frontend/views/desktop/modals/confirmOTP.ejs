
<!-- Enter OTP -->

<div class="modal fade" id="confirmOTP" tabindex="-1" role="dialog" aria-labelledby="confirmOTPLabel" aria-hidden="true">
    <div class="modal-dialog" role="document" style="margin-top:20px; width:570px;">
        <div class="modal-content" style="padding:20px 40px;">
            <div class="modal-header">
                <h6 class="modal-title" id="confirmOTPLabel" style="display: contents;"><%= langData.confirmLabel %></h6>
                <button type="button" class="close closeButton" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group">
                        <label for="otp" class="col-form-label"><%= langData.otp %></label>
                        <input type="text" class="" id="otp" placeholder="Enter OTP number was sent to your phone" />
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <div class="clearfix"></div>
                <button type="button" class="nextButton" id="confirmOtp"><%= langData.confirmButton %></button>
            </div>
        </div>
    </div>
</div>
<!-- End Enter OTP -->