				</div>
				<div class="conRight">
					<div class="citiesMap" id="mapIndustrialPark" style="width: 100%; height: 450px">

					</div>
					<script src="https://maps.googleapis.com/maps/api/js?mid=1cFDvE7JtCCxjrR7jk9_sqz3tsXU&key=AIzaSyC01xk6g1-fLTBjEZeRouhA9TOLt8kEZeU&libraries=places"></script>
					<script>
							function initMapIndustrialPark() {
								let lat = parseFloat("<%= pageData.address.latLng.lat %>");
								let lng = parseFloat("<%= pageData.address.latLng.lng %>");
								
								var mapIndustrialPark = new google.maps.Map(document.getElementById('mapIndustrialPark'), {
									zoom: 13,
									center: {lat: lat, lng: lng},
									maxZoom: 20
								});
								<% if (pageData.geometry && pageData.geometry.coordinates) { %> 
									var coords = JSON.parse("<%- JSON.stringify(pageData.geometry.coordinates) %>")[0];
									var coordsLatLng=[]; 

									for(var i=0; i < coords.length; i++) { 
										coordsLatLng.push({
											lng: parseFloat(coords[i][0]),
											lat:  parseFloat(coords[i][1])
										});
									}
									
									let bermudaTriangle = new google.maps.Polygon({
										paths: coordsLatLng,
										strokeColor: '#FF0000',
										strokeOpacity: 0.8,
										strokeWeight: 2,
										fillColor: '#FF0000',
										fillOpacity: 0.35
									});

									bermudaTriangle.setMap(mapIndustrialPark);
								<% } else { %>
									let markerIp = new google.maps.Marker({
										position: new google.maps.LatLng(lat, lng),
										title: "<%= pageData.name %>"
									});
									markerIp.setMap(mapIndustrialPark);
								<% } %>

								}

							google.maps.event.addDomListener(window, "load", initMapIndustrialPark);
					</script>
				</div>
			</div>
