						<script>
								function initMapProvinceCity() {
								var coords = JSON.parse("<%- JSON.stringify(pageData.geometryProvince) %>");
								let tempCenter = coords[0][0];
								let lat = parseFloat(tempCenter[parseInt(tempCenter.length/2)][1]);
								let lng = parseFloat(tempCenter[parseInt(tempCenter.length/2)][0]);
								
								var mapProvinceCity = new google.maps.Map(document.getElementById('mapProvinceCity'), {
									zoom: 8,
									center: {lat: lat, lng: lng},
									maxZoom: 20
								});

								let coordsLatLng=[]; 

								for(let i=0; i < coords.length; i++) {
									for (let j=0; j < coords[i].length; j++) {
										coordsLatLng = [];
										tempCoords = coords[i][j];
										for (let z=0; z < tempCoords.length; z++) {
											coordsLatLng.push({
												lng: parseFloat(tempCoords[z][0]),
												lat:  parseFloat(tempCoords[z][1])
											});
										}
										let bermudaTriangle = new google.maps.Polygon({
											paths: coordsLatLng,
											strokeColor: '#FF0000',
											strokeOpacity: 0.8,
											strokeWeight: 2,
											fillColor: '#FF0000',
											fillOpacity: 0.35
										});

										bermudaTriangle.setMap(mapProvinceCity);
									}
								}

							}

							google.maps.event.addDomListener(window, "load", initMapProvinceCity);
						</script>
