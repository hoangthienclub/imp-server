		$(function() {
			$.ajax({
				url: "http://vietnampropertyhub.vn/map/property/" + "<%= pageData.provinceId[0] %>",
				method: "get",
				success: function (data) {
					var listPropertyHubHTML = $("#listPropertyHub");
					if (data.length > 0 && data[0].length > 0) {
						$("#totalProperty").text(data[0].length);

						for (let i = 0; i < data[0].length; i++) {
							let propertyData = data[0][i];
							let purpose = "";

							if (propertyData.tenure && propertyData.tenure.lease == true)
								purpose = "For Lease";
							else if (propertyData.tenure && propertyData.tenure.sale == true)
								purpose = "For Sale";

							listPropertyHubHTML.append(`
							<div class="propertyListItem">
								<div class="propertyListItemTop">
									<a target="_blank" href="http://vietnampropertyhub.com/detail/` + propertyData.url + `">
										<img src="` + propertyData.image + `" width="100%">
										<h6>` + propertyData.name + `</h6>
									</a>
								</div>
								<div class="propertyListItemBot">
									<h6>` + propertyData.price.text + `</h6>
									<div class="botB">
										<p>Type</p>
										<h7>` + propertyData.proptype + `</h7>											
									</div>
									<div class="botB">
										<p>Area</p>
										<h7>` + propertyData.area.text + `</h7>											
									</div>
									<div class="botB">
										<p>Location</p>
										<h7>` + propertyData.district + `, ` + propertyData.province + `</h7>											
									</div>
									<div class="botB">
										<p>Purpose</p>
										<h7>` + purpose + `</h7>											
									</div>
								</div>
							</div>
						`);
						}
					}
				}
			})
		});
	</script>

	<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/2.0.0/TweenMax.min.js"></script>
	<script src="public/js/waypoints.min.js"></script>
	<script src="public/js/jquery.counterup.min.js"></script>
	<script src="public/js/desktop.js"></script>

</body>

</html>
